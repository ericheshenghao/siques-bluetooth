<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="568.0" prefWidth="776.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.siques.controller.BluetoothConnector">
   <children>
      <Pane disable="true" layoutX="243.0" layoutY="97.0" prefHeight="239.0" prefWidth="208.0" style="-fx-background-color: white;" />
      <Button layoutX="420.0" layoutY="45.0" mnemonicParsing="false" text="断开" />
      <Button fx:id="connectBtn" layoutX="340.0" layoutY="42.0" mnemonicParsing="false" onAction="#doConnect" text="连接" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="sendBtn" layoutX="243.0" layoutY="417.0" mnemonicParsing="false" onAction="#doSend" text="发送" />
      <ListView fx:id="toothList" editable="true" prefHeight="568.0" prefWidth="214.0" />
      <ProgressIndicator fx:id="progress" layoutX="96.0" layoutY="240.0" progress="0.0" />
      <Button fx:id="refreshBtn" layoutX="236.0" layoutY="479.0" mnemonicParsing="false" onAction="#doRefresh" style="-fx-border-radius: 50%;" text="刷新列表">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <TextArea fx:id="textSend" layoutX="234.0" layoutY="363.0" prefHeight="39.0" prefWidth="261.0" />
      <TextArea fx:id="textReceive" layoutX="522.0" layoutY="363.0" prefHeight="39.0" prefWidth="240.0" />
   </children>
</AnchorPane>
